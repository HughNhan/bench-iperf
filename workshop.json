{
    "workshop": {
        "schema": {
            "version": "2020.03.02"
        }
    },
    "userenvs": [
	    {
	        "name": "default",
	        "requirements": [
                "iperf_src",
                "debug"
	        ]
	    }
    ],
    "requirements": [
        {
            "name": "debug",
            "type": "distro",
            "distro_info": {
                "packages": [
                    "ethtool",
                    "net-tools",
                    "tcpdump"
                ]
            }
        },
	    {
	        "name": "iperf_src",
	        "type": "manual",
	        "manual_info": {
		    "commands": [
		        "git clone https://github.com/HughNhan/iperf.git /root/iperf",
		        "cd /root/iperf; git checkout udp-delay",
		        "cd /root/iperf; CFLAGS=\"-ggdb\" ./configure",
		        "cd /root/iperf; touch 1045",
		        "cd /root/iperf; make",
		        "cd /root/iperf; make install",
		        "cd /root/iperf; ldconfig",
		        "/usr/local/bin/iperf3 -v"
		            ]
	        }
        }
    ]
}
